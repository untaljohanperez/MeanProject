
<!DOCTYPE html>
<html>
<head>
	<title>Process Manager</title>
	<link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
</head>
<body ng-app="mySocketApp" class="wrapper" >
	<div class="container" ng-controller="ProcessManagerController as processControl">
		<div class="well" ng-show="processList.length > 0">
			<div class="row">
				<div class="col-md-12">
					<h1 class="text-primary">Queue Process</h1>
				</div>		
			</div>
			<div class="row">
				<div class="col-md-8 col-md-offset-2">
					<div class="table-responsive">
						<table class="table table-hover table-condensed" id="processQueue">
							<thead>
								<tr>
									<th>Id</th>
									<th>Process</th>
									<th>Processing Time (s)</th>
									<th>Remaining time (s)</th>
									<th>Status</th>
								</tr>
							</thead>
							<tbody id="processList">
								<tr ng-repeat="process in processList track by $index">
									<td>{{process.id}}</td>
									<td class="text-info">{{process.process}}</td>
									<td>{{process.processingTime}}</td>
									<td>{{process.remainingTime}}</td>
									<td>{{process.status}}</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-md-4">
					<button class="btn btn-warming" ng-click="startProcessing()">Start Processing</button>
				</div>
				<div class="col-md-2 col-md-offset-6">
					<button class="btn btn-danger" ng-click="clearProcessList()">Clear List</button>
				</div>	
			</div>	
		</div>
	</div>

	<script src="/scripts/jquery/dist/jquery.js"></script>
	
	<script src="/scripts/angular/angular.min.js"></script>

	<script src="https://cdn.socket.io/socket.io-1.4.5.js"></script> 
	
	<script src="/scripts/socket.io/lib/socket.js"></script> 
	
		<script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>  

	<script src="/javascripts/socketAngular.js"></script>
	
	<script src="/scripts/angular/angular.min.js"></script>
	
	<script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

	<script src="/javascripts/angularApp.js"></script> 
	
